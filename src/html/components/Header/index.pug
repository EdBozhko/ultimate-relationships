header(class='main-header')
    div(class='timer-box' dir='ltr')
        div(class='timer')
            span(class='timer__dozens_of_minutes') 0
            span(class='timer__minutes') 0
            span(class='timer__divider') :
            span(class='timer__dozens_of_seconds') 0
            span(class='timer__seconds') 0
    div(class='main-header__progress-container')
        div(class='progress-item')
            div(class='progress-item__img-wrapper')
                img(src="@img/icons/models/tiffany.png" alt="" width=40 height=40 class='progress-item__img --avatar --active')
                img(src="@img/icons/splashes.svg" alt="" width=40 height=40 class='progress-item__img --button')
                span(class='progress-item__name') Make her CUM
            div(class='progress-bar')
                div(class='progress')